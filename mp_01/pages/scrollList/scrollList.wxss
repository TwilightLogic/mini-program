/* pages/scrollList/scrollList.wxss */

.container1 {
  border: 1px solid lightblue;
  width: 100px;
  height: 120px;
}

.container1 view {
  width: 100px;
  height: 100px;
  text-align: center;
  line-height: 100px;
}

.container1 view:nth-child(1) {
  background-color: lightcyan;
}

.container1 view:nth-child(2) {
  background-color: lightgray;
}

.container1 view:nth-child(3) {background-color: lightsalmon;}